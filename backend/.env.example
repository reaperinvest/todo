# Copy this file to `.env` and fill your real credentials

# Example for local Postgres
# DATABASE_URL=postgres://postgres:YOUR_PASSWORD@localhost:5432/todo

# Or discrete settings instead of DATABASE_URL
# PGHOST=localhost
# PGUSER=postgres
# PGPASSWORD=YOUR_PASSWORD
# PGDATABASE=todo
# PGPORT=5432

# Optional: change API port
# PORT=4000

